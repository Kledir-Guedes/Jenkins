
pipeline {
    agent any    
    parameters{
            choice( name: 'Environment',
               choices:'lab\nhomol\nprod',
               description: 'Which environment do you want to run the job in?' )
          }
          
    stages {
        stage('Build') {
            steps {
            echo 'inicio'              
                               
            }
        }
        stage('tag release') {
            steps {
              sh 'echo "tag=1.1"'
              echo "${tag}"              
                
            }
        }
        stage('Deploy') {
            steps {
            echo 'fim'
                
            }
        }
    }
}
