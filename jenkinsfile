pipeline {
    agent any    
    parameters{
            string(
               name: 'ENV',
               defaultValue:'lab')
            string(
                name: 'tag',
                defaultValue:"$TAG")
          }
    stages {
        stage('Build') {
            steps {
            echo "TAG=1.1"
            
                
                               
            }
        }
        stage('tag release') {
            steps {
               echo "${TAG}"
               
                
            }
        }
        stage('Deploy') {
            steps {
            echo 'fim'
                
            }
        }
    }
}
