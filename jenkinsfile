pipeline {
    agent any
    parameters{
            string(
               name: 'ENV',
               defaultValue:'lab')
            string(
                name: 'tag',
                defaultValue:"$TAG")
          }
    stages {
        stage('Build') {
            steps {
            sh 'TAG=1.1'
            
                
                               
            }
        }
        stage('tag release') {
            steps {
               echo '$TAG1
                
            }
        }
        stage('Deploy') {
            steps {
            echo 'fim'
                
            }
        }
    }
}
