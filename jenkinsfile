
pipeline {
    agent any    
    parameters{
            choice( name: 'Environment',
               choices:'lab\nhomol\nprod'
               description: 'Which environment do you want to run the job in?' )
          }
          
    stages {
        stage('Build') {
            steps {
            sh "echo export tag=1.1"              
                               
            }
        }
        stage('tag release') {
            steps {
               echo "${tag}"              
                
            }
        }
        stage('Deploy') {
            steps {
            echo 'fim'
                
            }
        }
    }
}
