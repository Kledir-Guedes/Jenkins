
pipeline {
    agent any    
    parameters{
            choice( name: 'Environment',
               choices:'lab\nhomol\nprod',
               description: 'Which environment do you want to run the job in?' )
               
            string(name: 'tag',
                defaltvalue: "1 + 1"
          }
          
    stages {
        stage('Build') {
            steps {
                echo 'inicio'  
                
            }
        }
        stage('tag release') {
            steps {
                echo 'teste'    
            }
        }
         stage('Deploy') {
            def tag = 1.1 
            steps {
                echo "${tag}"                
                echo 'fim'                
            }
        }       
    }
}
