
pipeline {
    agent any    
    parameters{
            choice( name: 'Environment',
               choices:'lab\nhomol\nprod',
               description: 'Which environment do you want to run the job in?' )
           }
        
    stages {
        when {
                // Only say hello if a "greeting" is requested
                expression { params.Environment == 'lab' }
            } 
        stage('Build') {
            steps {
                echo 'inicio lab'                  
            }
        }
      }
}

