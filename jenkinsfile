
pipeline {
    agent any    
    parameters{
            string(
               name: 'ENV',
               defaultValue:'lab')
          }
          
    stages {
        stage('Build') {
            steps {
            sh "echo export tag=1.1"              
                               
            }
        }
        stage('tag release') {
            steps {
               echo ${tag}               
                
            }
        }
        stage('Deploy') {
            steps {
            echo 'fim'
                
            }
        }
    }
}
