
pipeline {
    agent any    
    parameters{
            choice( name: 'Environment',
               choices:'lab\nhomol\nprod',
               description: 'Which environment do you want to run the job in?' )
           }
          
    stages {
        if (env.Environment == "lab") {
        stage('Build') {
            steps {
                echo 'inicio lab'                  
            }
        }
        stage('tag release') {
          steps {
                              
                echo 'teste lab'
                                 
            }
        }
         stage('Deploy') {
           steps {             
                echo "fim lab"               
            }
        }
      } else {
           stage('Build') {
            steps {
                echo 'inicio prod'                  
            }
        }
      }
    }
}

