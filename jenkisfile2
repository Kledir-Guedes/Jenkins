pipeline {
    agent any
    environment {
        DISABLE_AUTH = 'true'
        DB_ENGINE    = ''
    }
    stages {
        stage('Example Username/Password1') {
           steps {
               sh 'echo "DB_ENGINE=1.1"'
               echo "Database engine is ${DB_ENGINE}"
               echo "DISABLE_AUTH is ${DISABLE_AUTH}"
            }
        }
     }
}
